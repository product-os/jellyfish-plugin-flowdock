{
  "name": "@balena/jellyfish-plugin-flowdock",
  "description": "Flowdock Jellyfish Plugin",
  "version": "1.0.71",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-flowdock"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest test/unit",
    "integration": "jest --runInBand --bail test/integration",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-plugin-base": "^2.1.188",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^15.0.7",
    "@balena/jellyfish-config": "^1.3.3",
    "@balena/jellyfish-environment": "^4.2.8",
    "@balena/jellyfish-plugin-default": "^18.0.1",
    "@balena/jellyfish-test-harness": "^6.0.1",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/lru-cache": "^5.1.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.5.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.21.3"
  },
  "peerDependencies": {
    "@balena/jellyfish-action-library": "^15.0.7",
    "@balena/jellyfish-environment": "^4.2.8",
    "@balena/jellyfish-plugin-default": "^18.0.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-07-10T20:47:59.927Z"
  }
}
