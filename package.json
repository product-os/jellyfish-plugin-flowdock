{
  "name": "@balena/jellyfish-plugin-flowdock",
  "description": "Flowdock Jellyfish Plugin",
  "version": "5.0.42",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-flowdock"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --bail --runInBand test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-worker": "^31.0.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0"
  },
  "devDependencies": {
    "@balena/jellyfish-environment": "^12.2.0",
    "@balena/jellyfish-plugin-default": "^27.9.3",
    "@balena/jellyfish-plugin-product-os": "^7.1.3",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/lru-cache": "^5.1.1",
    "deplint": "^1.1.3",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "nock": "^13.2.7",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "ts-jest": "^28.0.5",
    "typedoc": "^0.23.7",
    "typescript": "^4.7.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-07-15T02:16:12.016Z"
  }
}
