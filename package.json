{
  "name": "@balena/jellyfish-plugin-flowdock",
  "description": "Flowdock Jellyfish Plugin",
  "version": "1.0.156",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-flowdock"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --runInBand --bail test/integration",
    "test:compose": "DOCKER_BUILDKIT=1 docker build -t balena/jellyfish-sut:latest --secret id=npmrc,src=.balena/secrets/npmrc.txt . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-plugin-base": "^2.1.244",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-action-library": "^15.1.22",
    "@balena/jellyfish-config": "^1.5.1",
    "@balena/jellyfish-environment": "^5.0.3",
    "@balena/jellyfish-plugin-default": "^21.1.0",
    "@balena/jellyfish-test-harness": "^8.1.4",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/lru-cache": "^5.1.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.5"
  },
  "peerDependencies": {
    "@balena/jellyfish-action-library": "^15.1.22",
    "@balena/jellyfish-environment": "^5.0.3",
    "@balena/jellyfish-plugin-default": "^21.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-10-04T06:50:58.894Z"
  }
}
